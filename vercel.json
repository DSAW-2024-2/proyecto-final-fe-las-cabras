{
    "version": 2,
    "builds": [
      {
        "src": "vite.config.ts",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      { "src": "/static/(.*)", "dest": "/static/$1" },
      { "src": "/(.*)", "dest": "/index.html" }
    ],
    "static": {
      "cacheControl": "public, max-age=31536000, immutable"
    }
  }
  